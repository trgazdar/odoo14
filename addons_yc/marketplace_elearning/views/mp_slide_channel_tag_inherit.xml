<odoo><data>
        <record id="elearning_slide_channel_tag_inherited" model="ir.ui.view">
            <field name="name">elearning.slide.channel.tag.inherited</field>
            <field name="model">slide.channel.tag</field>
            <field name="inherit_id" ref="website_slides.slide_channel_tag_view_tree"/>
            <field name="arch" type="xml">
              <xpath expr="//field[@name='sequence']" position="replace" >

                <field name="sequence" widget="handle" groups="odoo_marketplace.marketplace_officer_group"/>
                   
              </xpath>
            </field>
        </record>
        <record id="elearning_slide_channel_tag_group_inherited" model="ir.ui.view">
            <field name="name">elearning.slide.channel.tag.group.inherited</field>
            <field name="model">slide.channel.tag.group</field>
            <field name="inherit_id" ref="website_slides.slide_channel_tag_group_view_tree"/>
            <field name="arch" type="xml">
              <xpath expr="//field[@name='sequence']" position="replace" >

                <field name="sequence" widget="handle" groups="odoo_marketplace.marketplace_officer_group"/>
                   
              </xpath>
            </field>
        </record>

    </data>
</odoo>
